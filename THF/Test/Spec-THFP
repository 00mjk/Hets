logic THF.THFP

spec test =

thf(type_a,type,( a : ($tType * ($tType > ($tType * $tType))) )).

thf(type_c,type,( c : ($tType * ($tType * $tType)) )).

thf(test_const,type,( test_const : c > a )).

thf(test_tuple,type,( test_tuple: c )).

thf(ax_in,axiom,( ((pr_2 @ (test_const @ test_tuple)) @ b) = [b,b] )).

thf(th_in,conjecture,( ((pr_2 @ (test_const @ test_tuple)) @ b) = [b,b] )).
