imports
{
import TokenDef(Pos)
}

DATA HsTokensRoot
   | HsTokensRoot tokens : HsTokens

TYPE HsTokens = [HsToken]

DATA HsToken
   | AGLocal   var   : {String}
               pos   : {Pos}

   | AGField   field : {String}
               attr  : {String}
               pos   : {Pos}

   | HsToken   value : {String}
               pos   : {Pos}
   | CharToken value : {String}
               pos   : {Pos}
   | StrToken  value : {String}
               pos   : {Pos}
   | Err       mesg  : {String}
               pos   : {Pos}