var s,t: Type;

class b = { x . x < s}  %% assume s < t

class a = { x . x < t} 

class a < b        %% a is an alias class

class a = {y . y < t }  %% ok (but only for simple tokens)

class a = {y . y < s }  %% wrong redefinition

class c,d

class a = (c,d)      %% wrong redefinition

class e = a          %% ok intersection

class h < e          %% ok 

class h < (a,b)      %% ok 

class e = { x . x < t }  %% wrong because e is intersection class
 
 

