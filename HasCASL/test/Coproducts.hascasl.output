type  Sum a b
ops   inl : a -> Sum a b;
      inr : b -> Sum a b;
      sumcase : (a ->? c) -> (b ->? c) -> Sum a b ->? c;
      bot : ? a
vars  f : a ->? c; g : b ->? c; h : Sum a b ->? c
. h = sumcase f g
