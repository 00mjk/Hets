vars  x : a; y : b; z : Sum a b
ops   outl : Sum a b -> a;
      outr : Sum a b -> b
. outl = (sumcase \ x : a . x) \ y : b . bot
. outr = (sumcase \ x : a . bot) \ y : b . y;
type  Bool := Sum Unit Unit
vars  p : Bool; x, w : a
ops   True, False : Bool;
      if__then__else__ : Bool * a * a -> a
. True = inl
. False = inr
. if p then x else w = ((sumcase \ () . x) \ () . w) p;
