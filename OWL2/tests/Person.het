logic OWL

spec person =
    ObjectProperty: hasSSN
    ObjectProperty: hasName
    DataProperty: hasAge

    Datatype: natural
        EquivalentTo: positiveInteger

    Datatype: Years
        EquivalentTo: natural [<150]

    Individual: Ann
        Types: hasAge some integer

    Class: Person
        HasKey: hasSSN, hasName

end
