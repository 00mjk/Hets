
library SyntaxTest

logic CASL_DL

spec X0 = {}

spec X1 = 
     sort T1,T2,T3 < Thing
     pred R1 : T1 * T2
     pred R1 : Thing * Thing
     op i1 : T2
     forall x : T1
     . x in T3 <=> minCardinality[(pred R1: T1 * T2)](x,10)
