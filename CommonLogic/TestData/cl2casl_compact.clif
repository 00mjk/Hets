(cl-text prop
  (thisMustBeTrue)
  (and (a) (b) (not (c)))
  (or (d) (e) (not (f)))
)

(cl-text pred
  (P g)
  (Q h)
  (P g h)
)

(cl-text quant
  (forall (x y z) (and (P x) (P y) (not (P z))))
  (exists (x) (P x))
)

(cl-text compact
  (P g)
  (Q P)
  (forall (P) (P h))
)

(cl-text func
  (= (F g) h)
  (= g (G h i j))
  (forall (H I) (= (H g) (I h i)))
  (= ((J ((K g) h) i) j) k)
)
