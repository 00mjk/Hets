logic LF
spec SP1 =
  o : type.
  ded : o -> type.
  i : type.
  p : o.
  q : o = p.
  f : i -> o.

  . ded q
end

spec SP2 = 
  o : type.
  p : o.
  a : o = p.
  q = p.
end

spec SP = SP1 and SP2

spec SP' = SP then
  b : o.
  
  .ded b
end

spec SP3 = 
nat : type.
t1 : nat.
t3 : nat.
end

spec SP4 =
nat : type.
s1 : nat.
s2 : nat.
s3 : nat.
plus : nat -> nat -> nat.
end

view V1 : SP3 to SP4 = t1 |-> s1, t3 |-> plus s1 s3
