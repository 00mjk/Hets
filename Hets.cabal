name: Hets
version: 0.87
Cabal-Version: >= 1.2.2
License-File: LICENSE.txt
Description: The Heterogeneous Tool Set
build-type: Simple
build-depends:
    base >= 3.0.1.0
  , filepath
  , ghc-prim
  , array >= 0.1.0.0
  , containers >= 0.1.0.1
  , directory >= 1.0.0.0
  , pretty >= 1.0.0.0
  , process >= 1.0.0.0
  , haskell98 >= 1.0.1.0
  , mtl >= 1.1.0.0
  , parsec >= 2.1.0.0
  , regex-compat >= 0.71.0.1
  , fgl >= 5.4.1.1
  , time >= 1.1.2.0
  , old-time >= 1.0.0.0
  , unix >= 2.3.0.0
  , network >= 2.1.0.0
  , Shellac >= 0.9.5
  , Shellac-editline >= 0.9.5
  , HaXml >= 1.13.2
  , HaXml <= 1.13.3
  , hxt >= 8.0.0
  , hxt-filter >= 8.0.0
  , uni-htk >= 2.0
  , uni-util
  , uni-posixutil
  , uni-events
  , uni-reactor
  , uni-graphs >= 2.0
  , uni-uDrawGraph >= 2.0
  , HAIFA >= 0.11
  , syb-generics == 2.9
  , programatica == 1.0
  , tabular >= 0.1
  , gtk >= 0.9.13
  , glade >= 0.9.13
executable: hets
main-is: hets.hs
extensions: OverlappingInstances
ghc-options: -fglasgow-exts -DUNI_PACKAGE -DUNIVERSION2 -DHXTFILTER
  -DCASLEXTENSIONS -DPROGRAMATICA -DHAXML_PACKAGE -DTABULAR_PACKAGE
  -DSHELLAC -DEDITLINE -DGTKGLADE
